    "#     mouse = surface_targets\n",
    "#     rat = surface_indexes\n",
    "#     zoo = s_ref_list\n",
    "    final_results = {}\n",
    "    for regularization in reg:\n",
    "        for feature in cat:        \n",
    "            for target in mouse:\n",
    "                for index in rat:\n",
    "                    if \"kernel\" in feature:\n",
    "                        for i, n_ref in enumerate(zoo):\n",
    "                            kMM = dog[feature][i]\n",
    "                            kernel_feature = cat[feature][i]\n",
    "                            total_target = mouse[target]\n",
    "                            cv_train_loss, cv_test_loss, train_loss, test_loss = find_kernelreg_CV(kernel_feature,\n",
    "                            total_target, xdos, kMM, regularization, rat[index][0], rat[index][1], cv = 2)\n",
    "                            final_results['{}_{}_{}_{}_{}'.format(feature, n_ref ,target, index, regularization)] = [cv_train_loss, cv_test_loss, train_loss, test_loss]\n",
    "                    else:\n",
    "                        soap_feature = cat[feature]\n",
    "                        total_target = mouse[target]\n",
    "                        cv_train_loss, cv_test_loss, train_loss, test_loss = find_reg_CV(cat[feature], total_target,\n",
    "                        xdos, regularization, rat[index][0], rat[index][1], cv = 2)\n",
    "                        final_results['{}_{}_{}_{}'.format(feature ,target, index, regularization)] = [cv_train_loss, cv_test_loss, train_loss, test_loss]\n",
    "                \n",
    "    return final_results\n",
    "\n",
