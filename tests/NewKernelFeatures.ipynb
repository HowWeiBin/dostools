    "\n",
    "                    for i, n_ref in enumerate((kernel_refs)):\n",
    "                        reg_loss = []\n",
    "                        for regularization in reg:\n",
    "                            reg_loss.append(reg_dict['{}_{}_{}_{}_{}'.format(feature, n_ref ,target, index, regularization)][1])\n",
    "                        reg_loss = torch.tensor(reg_loss)\n",
    "                        _, r = torch.min(reg_loss, 0)\n",
    "                        optimal_reg_dict[feature][target][index][n_ref] = reg[r]\n",
